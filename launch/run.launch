<launch>

<arg name="input_topic"  default="/ip_basic/input"/>
<arg name="output_topic" default="/ip_basic/output"/>
<arg name="fill_type"    default="fast"/>
<arg name="extrapolate"  default="false"/>
<arg name="blur_type"    default="bilateral"/>

<node pkg="ip_basic" type="ip_basic_node.py" name="ip_basic" output="screen">
    <remap from="input"  to="$(arg input_topic)"/>
    <remap from="output" to="$(arg output_topic)"/>
    <param name="fill_type"   value="$(arg fill_type)"/>
    <param name="extrapolate"   value="$(arg extrapolate)"/>
    <param name="blur_type"   value="$(arg blur_type)"/>
</node>

</launch>
